# ========================================
# S3-Compatible Environment Configuration
# ========================================

# Application URLs
NEXT_PUBLIC_APP_URL=https://your-domain.com
BETTER_AUTH_URL=https://your-domain.com

# Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password
POSTGRES_DB=simstudio
POSTGRES_PORT=5432

# Authentication
BETTER_AUTH_SECRET=your_auth_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# OAuth Providers
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# Email Service
RESEND_API_KEY=your_resend_api_key

# External Services
OLLAMA_URL=http://localhost:11434
SOCKET_SERVER_URL=http://localhost:3002
NEXT_PUBLIC_SOCKET_URL=http://localhost:3002

# AI API Keys
OPENAI_API_KEY=your_openai_api_key
OPENAI_API_KEY_1=your_openai_api_key_1
OPENAI_API_KEY_2=your_openai_api_key_2
OPENAI_API_KEY_3=your_openai_api_key_3
ANTHROPIC_API_KEY_1=your_anthropic_api_key_1
ANTHROPIC_API_KEY_2=your_anthropic_api_key_2
ANTHROPIC_API_KEY_3=your_anthropic_api_key_3
XAI_API_KEY=your_xai_api_key
XAI_API_KEY_1=your_xai_api_key_1
XAI_API_KEY_2=your_xai_api_key_2
XAI_API_KEY_3=your_xai_api_key_3

# ========================================
# S3 Configuration
# ========================================

# AWS S3 Bucket Settings
S3_BUCKET_NAME=your-s3-bucket-name
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# Optional S3 Settings (for custom S3-compatible services)
AWS_S3_ENDPOINT=
AWS_S3_FORCE_PATH_STYLE=false

# Public S3 URLs (for frontend)
NEXT_PUBLIC_S3_BUCKET=your-s3-bucket-name
NEXT_PUBLIC_S3_REGION=us-east-1

# ========================================
# Build Configuration
# ========================================

# Docker Build Context
DOCKER_BUILD=1
NEXT_TELEMETRY_DISABLED=1
VERCEL_TELEMETRY_DISABLED=1
